{
  "id": null,
  "uid": "osm-notes-analytics-datamarts",
  "title": "Analytics Datamarts Overview",
  "tags": ["osm", "monitoring", "analytics", "datamarts"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "panels": [
    {
      "id": 1,
      "title": "Datamart Status",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "PostgreSQL"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT labels->>'datamart' AS datamart, CASE WHEN metric_value = 1 THEN 'Running' ELSE 'Not Running' END as status FROM metrics WHERE component = 'analytics' AND metric_name = 'datamart_process_running' ORDER BY timestamp DESC LIMIT 3",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "Running": { "text": "Running", "color": "green" },
                "Not Running": { "text": "Not Running", "color": "red" }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" },
              { "value": 1, "color": "green" }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Datamart Freshness",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 6, "y": 0 },
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "PostgreSQL"
          },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT timestamp AS time, labels->>'datamart' AS datamart, metric_value AS value FROM metrics WHERE component = 'analytics' AND metric_name = 'datamart_freshness_seconds' AND $__timeFilter(timestamp) ORDER BY timestamp, datamart",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 86400, "color": "yellow" },
              { "value": 172800, "color": "red" }
            ]
          }
        }
      },
      "transformations": [
        {
          "id": "groupBy",
          "options": {
            "fields": {
              "datamart": {
                "aggregations": [],
                "operation": "groupby"
              }
            }
          }
        }
      ]
    },
    {
      "id": 3,
