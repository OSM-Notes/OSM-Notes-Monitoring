          "rawQuery": true,
          "rawSql": "SELECT timestamp AS time, labels->>'datamart' AS datamart, metric_value AS value FROM metrics WHERE component = 'analytics' AND metric_name = 'datamart_execution_duration_seconds' AND $__timeFilter(timestamp) ORDER BY timestamp, datamart",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 0
        }
      },
      "transformations": [
        {
          "id": "groupBy",
          "options": {
            "fields": {
              "datamart": {
                "aggregations": [],
