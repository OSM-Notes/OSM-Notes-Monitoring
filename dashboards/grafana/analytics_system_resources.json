      "id": 1,
      "title": "ETL CPU Usage (%)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [{
        "datasource": {
          "type": "postgres",
          "uid": "PostgreSQL"
        },
        "editorMode": "code",
        "format": "time_series",
        "rawQuery": true,
        "rawSql": "SELECT timestamp AS time, metric_value AS value FROM metrics WHERE component = 'ANALYTICS' AND metric_name = 'etl_cpu_usage_percent' AND $__timeFilter(timestamp) ORDER BY timestamp",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 70, "color": "yellow"},
              {"value": 90, "color": "red"}
            ]
          }
        }
      }
    },
