{
  "id": null,
  "uid": "osm-notes-analytics-dwh",
  "title": "Analytics DWH Performance",
  "tags": ["osm", "monitoring", "analytics", "dwh", "database"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "panels": [
    {
      "id": 1,
      "title": "DWH Schema Size",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "PostgreSQL"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT metric_value AS value FROM metrics WHERE component = 'ANALYTICS' AND metric_name = 'db_schema_size_bytes' ORDER BY timestamp DESC LIMIT 1",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 107374182400, "color": "yellow" },
              { "value": 1073741824000, "color": "red" }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Facts Partition Sizes",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 6, "y": 0 },
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "PostgreSQL"
          },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT timestamp AS time, labels->>'partition' AS partition, labels->>'year' AS year, metric_value AS value FROM metrics WHERE component = 'ANALYTICS' AND metric_name = 'db_facts_partition_size_bytes' AND $__timeFilter(timestamp) ORDER BY timestamp, partition",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "decimals": 2
        }
      },
      "transformations": [
        {
          "id": "groupBy",
          "options": {
            "fields": {
              "partition": {
                "aggregations": [],
                "operation": "groupby"
              }
            }
          }
        }
      ]
    },
    {
      "id": 3,
      "title": "Cache Hit Ratio (%)",
      "type": "gauge",
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 4 },
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "PostgreSQL"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT metric_value AS value FROM metrics WHERE component = 'ANALYTICS' AND metric_name = 'db_cache_hit_ratio_percent' ORDER BY timestamp DESC LIMIT 1",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 100,
          "unit": "percent",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" },
              { "value": 90, "color": "yellow" },
              { "value": 95, "color": "green" }
            ]
          }
        }
      }
    },
