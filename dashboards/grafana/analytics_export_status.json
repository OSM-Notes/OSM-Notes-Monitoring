          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT metric_value AS value, labels->>'status' AS status FROM metrics WHERE component = 'ANALYTICS' AND metric_name = 'export_validation_valid' AND labels->>'type' = 'json' ORDER BY timestamp DESC LIMIT 1",
          "refId": "A",
