        "rawQuery": true,
        "rawSql": "SELECT timestamp AS time, metric_value AS value FROM metrics WHERE component = 'ANALYTICS' AND metric_name = 'export_duration_seconds' AND $__timeFilter(timestamp) ORDER BY timestamp",
